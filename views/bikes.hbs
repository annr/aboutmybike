<div class="row">
  {{#each results}}
  <div class="col-md-4">
  <div class="card">
  <div class="card-photo-wrapper">
  <a href="/bike/{{id}}">
  <img class="card-photo" src="https://s3-us-west-1.amazonaws.com/amb-storage{{main_photo_path}}" alt="Fuji">
  </a>
  </div>
  <div class="card-block"><h4 class="card-title">{{model}} {{brand}}
  </h4>
  <p class="card-text"></p><a href="/bike/{{id}}">View Bike</a>
  </div>
  </div>
  </div>
  {{/each}}
</div>

        <br><br>
  <div class="bike-detail-container">
     <p><a class="btn btn-lg btn-youll-grow-out-of-it" href="/add" role="button">+ Add Your Bike</a></p>
  </div>
